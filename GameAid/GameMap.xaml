                </Grid>
            </Button>
            <Canvas Width="5"/>
            <Button ToolTip="Ellipse" VerticalAlignment="Center" Name="buttonEllipse" Click="drawing_button_Click" Padding="5,4,5,4">
                <Ellipse Width="18" Height="12" Fill="AntiqueWhite" Stroke="{x:Null}" Opacity=".8"></Ellipse>
            </Button>
            <Canvas Width="5"/>
            <Button ToolTip="Rectangle" VerticalAlignment="Center" Name="buttonRect" Click="drawing_button_Click" Padding="5,4,5,4">
                <Rectangle Width="20" Height="10" Fill="AntiqueWhite" Stroke="{x:Null}" Opacity=".8"></Rectangle>
            </Button>
            <Canvas Width="5"/>
            <Button ToolTip="Tile" VerticalAlignment="Center" Name="buttonTile" Click="drawing_button_Click" Padding="5,4,5,4">
                <l:Tile Width="40" Height="40" Opacity=".8">
                    <l:Tile.LayoutTransform>
                        <ScaleTransform ScaleX=".33" ScaleY=".33"/>
                    </l:Tile.LayoutTransform>
                </l:Tile>
            </Button>
            <Canvas Width="5"/>
            <Button ToolTip="Half Round Tile" VerticalAlignment="Center" Name="buttonHalfArc" Click="drawing_button_Click" Padding="5,4,5,4">
                <l:HalfArcTile Width="24" Height="12" Opacity=".8"/>
            </Button>
            
            <Canvas Width="5"/>
            <Button ToolTip="Quarter Round Tile" VerticalAlignment="Center" Name="buttonQuarterArc" Click="drawing_button_Click" Padding="5,4,5,4">
                <l:QuarterArcTile Width="12" Height="12" Opacity=".8"/>
            </Button>

            <Canvas Width="10"/>
            <Button ToolTip="Hard Snap" VerticalAlignment="Center" Name="buttonHardSnap" Click="buttonHardSnap_Click" Padding="5,4,5,4">
                <Grid>
                    <Line X1="0" X2="20" Y1="5" Y2="5" StrokeThickness="2" Stroke="Black"/>
                    <Line X1="0" X2="20" Y1="10" Y2="10" StrokeThickness="2" Stroke="Black"/>
                    <Line X1="5" X2="5" Y1="0" Y2="15" StrokeThickness="2" Stroke="Black"/>
                    <Line X1="15" X2="15" Y1="0" Y2="15" StrokeThickness="2" Stroke="Black"/>
                </Grid>
            </Button>

            <TextBlock Margin="5,0,0,0" VerticalAlignment="Center" Name="fontSampleText">Font Sample</TextBlock>
            <Button Margin="5,0,0,0" VerticalAlignment="Center" Name="buttonChangeFont" Click="buttonChangeFont_Click">Change</Button>
            <Button Margin="2,0,0,0" VerticalAlignment="Center" Name="buttonGive" Click="drawing_button_Click">Give to Main</Button>
            
        </StackPanel>
        <Canvas Height="1" DockPanel.Dock="Top" Background="{DynamicResource NormalBrush}"/>
        <Slider DockPanel.Dock="Left" Orientation="Vertical" Width="Auto" Name="zoomSlider"  ValueChanged="scrollZoom_ValueChanged" Value="5"/>
        <StackPanel DockPanel.Dock="Left" Orientation="Vertical">
            <Button Click="layer_Click" ToolTip="Background"   ><Button.LayoutTransform><ScaleTransform ScaleX=".75" ScaleY=".75"/></Button.LayoutTransform>0</Button>
            <Button Click="layer_Click" ToolTip="Rect"         ><Button.LayoutTransform><ScaleTransform ScaleX=".75" ScaleY=".75"/></Button.LayoutTransform>1</Button>
            <Button Click="layer_Click" ToolTip="Spare"        ><Button.LayoutTransform><ScaleTransform ScaleX=".75" ScaleY=".75"/></Button.LayoutTransform>2</Button>
            <Button Click="layer_Click" ToolTip="Text"         ><Button.LayoutTransform><ScaleTransform ScaleX=".75" ScaleY=".75"/></Button.LayoutTransform>3</Button>
            <Button Click="layer_Click" ToolTip="Spare"        ><Button.LayoutTransform><ScaleTransform ScaleX=".75" ScaleY=".75"/></Button.LayoutTransform>4</Button>
            <Button Click="layer_Click" ToolTip="Lines"        ><Button.LayoutTransform><ScaleTransform ScaleX=".75" ScaleY=".75"/></Button.LayoutTransform>5</Button>
            <Button Click="layer_Click" ToolTip="Doors/Objects"><Button.LayoutTransform><ScaleTransform ScaleX=".75" ScaleY=".75"/></Button.LayoutTransform>6</Button>
            <Button Click="layer_Click" ToolTip="Tokens"       ><Button.LayoutTransform><ScaleTransform ScaleX=".75" ScaleY=".75"/></Button.LayoutTransform>7</Button>
            <Button Click="layer_Click" ToolTip="Spare"        ><Button.LayoutTransform><ScaleTransform ScaleX=".75" ScaleY=".75"/></Button.LayoutTransform>8</Button>
            <Button Click="layer_Click" ToolTip="Spare"        ><Button.LayoutTransform><ScaleTransform ScaleX=".75" ScaleY=".75"/></Button.LayoutTransform>9</Button>
            <Button Margin="0,10,0,0" Click="layer_Click" ToolTip="Reset"        ><Button.LayoutTransform><ScaleTransform ScaleX=".75" ScaleY=".75"/></Button.LayoutTransform>c</Button>
            <TextBlock Margin="0,15,0,0" HorizontalAlignment="Center">Pen</TextBlock>
            <Rectangle ToolTip ="Pen/Stroke Color" StrokeThickness="1" Stroke="Black" HorizontalAlignment="Center" Name="strokeRect" Width="15" Height="15" Fill="Black" MouseDown="colorsample_MouseDown" ContextMenu="{DynamicResource StrokeMenu}"/>
            <TextBlock Margin="0,5,0,0" HorizontalAlignment="Center">Fill</TextBlock>
            <Rectangle ToolTip ="Brush/Fill Color" StrokeThickness="1" Stroke="Black" HorizontalAlignment="Center" Name="fillRect" Width="15" Height="15" Fill="Black" MouseDown="colorsample_MouseDown" ContextMenu="{DynamicResource FillMenu}"/>
            <TextBlock Margin="0,5,0,0" HorizontalAlignment="Center">Th</TextBlock>
            <Grid ToolTip ="Stroke Thickness" MouseDown="Thickness_Clicked" Background="White" Width="15" Height="15" ContextMenu="{DynamicResource ThicknessMenu}">
                <Line Name="thicknessSample" X1="0" X2="15" Y1="7.5" Y2="7.5" StrokeThickness="3" Stroke="Black" ContextMenu="{DynamicResource ThicknessMenu}"/>
            </Grid>
            <TextBlock Margin="0,5,0,0" HorizontalAlignment="Center">Op</TextBlock>
            <Grid>
            <Rectangle StrokeThickness="1" Stroke="Black" HorizontalAlignment="Center" Width="15" Height="15" ContextMenu="{DynamicResource OpacityMenu}"/>
            <Rectangle ToolTip ="Opacity" MouseDown="Opacity_Clicked" Fill="Black" HorizontalAlignment="Center" Name="opacityRect" Width="15" Height="15" ContextMenu="{DynamicResource OpacityMenu}"/>
            </Grid>

        </StackPanel>

        <ScrollViewer Name="scrollViewer" ScrollChanged="map_ScrollChanged"  HorizontalScrollBarVisibility="Visible" Background="{DynamicResource Shine1}">
            <Grid Name="canvas" MouseWheel="scrollViewer_MouseWheel" ClipToBounds="True">
                <Rectangle Name="rectHit" Fill="White" MouseMove ="canvas_MouseMove" MouseDown="canvas_MouseDown" MouseUp="canvas_MouseUp" Opacity="0" IsHitTestVisible="True" ClipToBounds="False"></Rectangle>
            </Grid>
        </ScrollViewer>
    </DockPanel>
</UserControl>
