        <Canvas Height="1" DockPanel.Dock="Top" Background="{DynamicResource NormalBrush}"/>
        <StackPanel DockPanel.Dock="Top" Orientation="Horizontal" MinHeight="30" Background="{DynamicResource Shine1}">
            <Canvas Width="5"/>
            <Button ToolTip="Move" VerticalAlignment="Center" Name="buttonMove" Click="drawing_button_Click" Padding="9,0,0,0">
                <Canvas Width="17" Height="17" VerticalAlignment="Top" HorizontalAlignment="Left">
                    <Path Margin="0,0,0,65.164" Fill="#FF000000" Stretch="Fill" Stroke="#FF000000" Data="M240,272 L240,112 352,224 304,224 352,312 326,326 277,237 240,272" Width="113" HorizontalAlignment="Left">
                        <Path.LayoutTransform>
                            <ScaleTransform ScaleX=".07" ScaleY=".07"/>
                        </Path.LayoutTransform>
                    </Path>
                </Canvas>
            </Button>
            <Canvas Width="5"/>
            <Button ToolTip="Pan" VerticalAlignment="Center" Name="buttonPan" Click="drawing_button_Click" Width="35">
                    <Canvas Width="17" Height="17" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="-5,-5,0,0">
                        <Canvas.LayoutTransform>
                            <TransformGroup>
                                <ScaleTransform ScaleX=".7" ScaleY=".7"/>
                            </TransformGroup>
                        </Canvas.LayoutTransform>
                        <Image Margin="14,8,0,0" Source="{DynamicResource Horizon_Image_Right}" VerticalAlignment="Top" HorizontalAlignment="Left" Height="100" Width="100">
                            <Image.LayoutTransform>
                                <TransformGroup>
                                    <ScaleTransform ScaleX=".1" ScaleY=".1"/>
                                    <SkewTransform AngleX="0" AngleY="0"/>
                                    <RotateTransform Angle="0"/>
                                    <TranslateTransform X="0" Y="0"/>
                                </TransformGroup>
                            </Image.LayoutTransform>
                        </Image>
                        <Image Margin="0,8,0,0" Source="{DynamicResource Horizon_Image_Right}" VerticalAlignment="Top" HorizontalAlignment="Left" Height="100" Width="100">
                            <Image.LayoutTransform>
                                <TransformGroup>
                                    <ScaleTransform ScaleX=".1" ScaleY=".1"/>
                                    <SkewTransform AngleX="0" AngleY="0"/>
                                    <RotateTransform Angle="-180"/>
                                    <TranslateTransform X="0" Y="0"/>
                                </TransformGroup>
